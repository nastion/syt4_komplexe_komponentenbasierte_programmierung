<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  

<hibernate-mapping>  
	<class name="pfuchs.syt4.westbahn.model.Reservierung">
		<id name="ID" access="field">
			<generator class="increment" />
		</id>
		<property name="datum" type="java.util.Date" access="field" />
		<property name="praemienMeilenBonus" type="int" access="field" />
		<property name="preis" type="int" access="field" />
		<component name="status" class="pfuchs.syt4.westbahn.model.StatusInfo" access="field" />
		<component name="zahlung" class="pfuchs.syt4.westbahn.model.Zahlung" access="field" />
		<one-to-one name="zug" class="pfuchs.syt4.westbahn.model.Zug" access="field" />
		<many-to-one name="strecke" class="pfuchs.syt4.westbahn.model.Strecke" access="field" />
		<many-to-one name="benutzer" class="pfuchs.syt4.westbahn.model.Benutzer" access="field" />
	</class>
</hibernate-mapping> 